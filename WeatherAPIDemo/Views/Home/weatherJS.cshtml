
@{
    ViewBag.Title = "WeatherJS";
}

<h2>WeatherJS</h2>

<p id="name"></p>
<p id="temp"></p>
<p id="humidity"></p>

<button>Get Weather</button>

<script>
    $(document).ready(function () {
        $("button").click(function () { 
            $.get("http://api.openweathermap.org/data/2.5/weather?q=Cleveland&appid=f9ef6ff16c06ec734352c3dab6dc88d5&units=imperial", function (response) {
                //response
                console.log(response);
                $("#name").text(response.name);
                $("#temp").text(response.main.temp);
                $("#humidity").text(response.main.humidity);
            });
        });
    });
</script>A